{
    "name": "lexal/form-submitter",
    "description": "Form submitter",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Lex",
            "email": "allapkov@gmail.com"
        }
    ],
    "keywords": [
        "form submitter",
        "stepped form submitter"
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.5",
        "roave/security-advisories": "dev-latest",
        "webimpress/coding-standard": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Lexal\\FormSubmitter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lexal\\FormSubmitter\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
